<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BaseRatingBar">
        <attr name="rating_numStars" format="integer"/>
        <attr name="rating_rating" format="float"/>
        <attr name="rating_starPadding" format="integer"/>
        <attr name="rating_drawableEmpty" format="reference"/>
        <attr name="rating_drawableFilled" format="reference"/>
